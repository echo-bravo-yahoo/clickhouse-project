{
  "name": "clickhouse-project",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "author": "Ashton Eby",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm-run-all --parallel start:**",
    "start:ext": "node ./ext/tmp/build/main.js",
    "start:src": "node ./tmp/build/main.js",
    "watch": "npm-run-all --parallel --print-label watch:**",
    "watch:ext": "tsc-watch --project ext --onSuccess \"node ./ext/tmp/build/main.js\"",
    "watch:src": "tsc-watch --project . --onSuccess \"node ./tmp/build/main.js\"",
    "build": "npm-run-all --parallel --print-label --aggregate-output build:**",
    "build:src": "tsc --project .",
    "build:ext": "tsc --project ext",
    "validate": ""
  },
  "dependencies": {
    "express": "^4.21.2",
    "lowdb": "^7.0.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "tsc-watch": "^6.2.1",
    "typescript": "^5.8.2"
  }
}
